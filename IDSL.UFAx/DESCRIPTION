Package: IDSL.UFAx
Type: Package
Title: Exhaustive Chemical Enumeration for United Formula Annotation
Version: 1.9
Authors@R: c(
    person(family = "Fakouri-Baygi", given = "Sadjad", email = "sadjad.fakouri-baygi@mssm.edu",
    role = "aut", comment = c(ORCID = "0000-0002-6864-6911")),
